<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignBottom="@+id/myMapView1" android:layout_marginLeft="20dp" android:background="@drawable/currentlocbutton" android:id="@+id/currentloc" android:layout_marginBottom="40dp"></Button>
</RelativeLayout>
